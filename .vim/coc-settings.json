{
  "jedi": {
    "enable": true,
    "executable": {
      "command": "jedi-language-server"
    }
  },
  "diagnostic-languageserver": {
    "filetypes": {
      "python": "flake8",
      "sh": "shellcheck"
    },
    "formatFiletypes": {
      "python": ["black", "isort"],
      "sh": "shfmt"
    }
  },
  "snnipets": {
    "userSnippetsDirector": "~/.vim/UltiSnips",
    "ultisnips": {
      "directories": ["~/.vim/UltiSnips"]
    }
  }
}
